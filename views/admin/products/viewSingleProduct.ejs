    <%- include('../partials/script'); -%>

    <script>

        $("#inputPassword2").keypress(function(){

            var search = $(this).val();

                $.ajax({
                    url: '<%=adminpath %>products/search',
                    type: 'POST',
                    data: {search: search},
                    success: function(data){

                        var obj = data.data;
                        $.each(obj,function(key,value){
                            
                            var pic = value.thumbnail;
                            var split = pic.split(",");
                            var picture = split[0];
                            // console.log(pic);

                            var html = `<div class="col-md-6 col-xl-3">
                            <div class="card-box product-box">

                                <div class="product-action">
                                    <a href="<%-adminpath%>products/edit/${value.id}" class="btn btn-success btn-xs waves-effect waves-light"><i class="mdi mdi-pencil"></i></a>
                                    
                                    <a href="<%-adminpath%>products/delete/${value.id}" onclick="return confirm('Are you sure you want to delete this item?');" class="btn btn-danger btn-xs waves-effect waves-light"><i class="mdi mdi-close"></i></a>
                                </div>

                                <div class="bg-light">
                                    

                                    <img src="/assets/images/products/${picture}" alt="product-pic" onError="this.onerror=null;this.src='/assets/images/products/no-image.jpg';" class="img-fluid" />
                                </div>

                                <div class="product-info">
                                    <div class="row align-items-center">
                                        <div class="col">
                                            <h5 class="font-16 mt-0 sp-line-1"><a href="ecommerce-product-detail.html" class="text-dark">${value.name}</a> </h5>
                                            <div class="text-warning mb-2 font-13">
                                                <i class="fa fa-star"></i>
                                                <i class="fa fa-star"></i>
                                                <i class="fa fa-star"></i>
                                                <i class="fa fa-star"></i>
                                                <i class="fa fa-star"></i>
                                            </div>
                                            <h5 class="m-0"> <span class="text-muted"> Stocks : 98 pcs</span></h5>
                                        </div>
                                        <div class="col-auto">
                                            <div class="product-price-tag">
                                                ${value.price}
                                            </div>
                                        </div>
                                    </div>
                                </div> 
                            </div> 
                        </div> `;

                        if(key == 0 || key == "0"){

                            $(".ajax_row").html(html);


                        }else{
                            $(".ajax_row").append(html);
                        }

                        });
                    }
                })

        });

    </script>

</body>

